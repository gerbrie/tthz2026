{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
	    "type": "composer",
            "url": "https://asset-packagist.org"	
        }
    ],
    "require": {
        "bower-asset/photoswipe": "^5",
        "composer/installers": "^1.9",
        "drupal/aggregator": "^2.2",
        "drupal/antibot": "^2.0",
        "drupal/asset_injector": "^2.10",
        "drupal/backup_migrate": "^5.0",
        "drupal/blazy": "^3.0",
        "drupal/color-color": "^1.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_perms": "^2.1",
        "drupal/consumers": "^1.22",
        "drupal/content_access": "^2.1@RC",
        "drupal/cookies": "^1.0",
        "drupal/core-composer-scaffold": "^11.1",
        "drupal/core-project-message": "^11.1",
        "drupal/core-recommended": "^11.1",
        "drupal/crop": "^2.3",
        "drupal/ctools": "^4.1",
        "drupal/d8w3css": "^2.0",
        "drupal/ds": "^3.18",
        "drupal/entity_browser": "^2.10",
        "drupal/entity_reference_revisions": "^1.11",
        "drupal/eu_cookie_compliance": "^1.24",
        "drupal/exif_orientation": "^1.1",
        "drupal/feeds": "^3.0@beta",
        "drupal/field_group": "^3.4",
        "drupal/field_permissions": "^1.3",
        "drupal/focal_point": "^2.1",
        "drupal/google_analytics": "^4.0",
        "drupal/honeypot": "^2.1",
        "drupal/image_widget_crop": "^3.0",
        "drupal/imce": "^3.0",
        "drupal/jquery_ui": "^1.6",
        "drupal/jquery_ui_datepicker": "^2.1",
        "drupal/jquery_ui_tabs": "^2.0",
        "drupal/jquery_ui_tooltip": "^2.0",
        "drupal/jsonapi_extras": "^3.23",
        "drupal/key_value_field": "^1.3",
        "drupal/login_destination": "^2.0@beta",
        "drupal/media_bulk_upload": "^3.0",
        "drupal/media_gallery": "^3.2",
        "drupal/media_library_form_element": "^2.0",
        "drupal/media_library_importer": "^2.1",
        "drupal/menu_per_role": "^1.5",
        "drupal/ng_lightbox": "^2.0",
        "drupal/one_time_login_link_admin": "^1.0",
        "drupal/paragraphs": "^1.17",
        "drupal/pathauto": "^1.11",
        "drupal/photos": "^6.1",
        "drupal/photoswipe": "^5.0",
        "drupal/queue_ui": "^3.2",
        "drupal/rdf": "^3.0@beta",
        "drupal/recaptcha": "^3.1",
        "drupal/recaptcha_element": "^1.0",
        "drupal/recaptcha_v3": "^2.0",
        "drupal/redirect": "^1.9",
        "drupal/simple_gmap": "^3.1",
        "drupal/simple_oauth": "^6.1",
        "drupal/smtp": "^1.2",
        "drupal/sophron": "^3.0",
        "drupal/theme_switcher": "^2.0",
        "drupal/token": "^1.13",
        "drupal/twig_debugger": "^1.1",
        "drupal/upgrade_status": "^4.3",
        "drupal/user_current_paths": "^2.0",
        "drupal/user_default_page": "^9.0",
        "drupal/views_slideshow": "^5.0",
        "drupal/w3css_paragraphs": "^1.0",
        "drupal/webform": "^6.3@beta",
        "drupal/webform_analysis": "^1.0@beta",
        "drupal/webform_deprecated": "^6.3@beta",
        "drupal/webform_submission_import": "^2.1",
        "drupal/webform_views": "^5.1",
        "drush/drush": "^13",
        "oomphinc/composer-installers-extender": "^2.0",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "require-dev": {
      "drupal/core-dev": "^11.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "oomphinc/composer-installers-extender": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true,
            "wikimedia/composer-merge-plugin": true
        },
        "audit": {
            "block-insecure": false
        }
    },
    "extra": {
        "patches": {
            "drupal/content_access": {
                    "Fix deprecated static trait method call": "patches/content_access-trait-fix.patch"
            }
        },  
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "installer-types": [
             "npm-asset",
             "bower-asset"
             ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/chosen": ["npm-asset/chosen-js"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "type:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
   
  }
}
